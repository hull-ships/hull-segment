{
  "name": "hull-segment",
  "description": "Install Segment in your site",
  "version": "0.2.0",
  "homepage": "https://github.com/hull-ships/hull-segment",
  "license": "MIT",
  "main": "dist/ship.js",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "^8.11.x",
    "yarn": "^1.6.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hull-ships/hull-segment"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-segment/issues"
  },
  "scripts": {
    "ngrok": "ngrok http 8082 --region eu --subdomain segment",
    "lint": "eslint server",
    "flow": "flow",
    "flow:stop": "flow stop",
    "clean": "./node_modules/.bin/rimraf dist; ./node_modules/.bin/rimraf lib",
    "build": "yarn clean && yarn build:server && yarn build:client",
    "build:client": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.js --progress --profile --colors",
    "build:server": "./node_modules/.bin/babel server -d lib",
    "start": "node -r newrelic ./lib",
    "dev": "./node_modules/.bin/nodemon -w server --exec ./node_modules/.bin/babel-node -- server",
    "start:dev": "yarn dev",
    "test": "yarn test:units",
    "test:modules": "npm outdated --depth=0",
    "test:units": "NODE_ENV=test jest ./tests",
    "update": "./node_modules/.bin/updtr",
    "watch": "./node_modules/.bin/watch 'clear && yarn lint -s && yarn test:units -s && yarn build:client -s' server src",
    "postinstall": "yarn build"
  },
  "keywords": [
    "hull",
    "ship",
    "hull-ship"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 10,
        "lines": 10,
        "statements": 10
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "dependencies": {
    "analytics-node": "^3.3.0",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "cache-manager-redis": "^0.6.0",
    "camelize": "^1.0.0",
    "connect": "^3.6.6",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "hull": "hull/hull-node#feature/enforce-code-style",
    "json-loader": "^0.5.7",
    "jwt-simple": "^0.5.1",
    "librato-node": "^5.0.2",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "newrelic": "^4.8.1",
    "raw-body": "^2.3.3",
    "rimraf": "^2.6.2",
    "webpack": "^4.18.1",
    "webpack-dev-middleware": "^3.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-flow-comments": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "prettier": "^1.14.2",
    "compose-middleware": "^5.0.0",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "flow-bin": "^0.80.0",
    "jest": "^23.6.0",
    "minihull": "^3.0.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "sinon": "^6.3.1",
    "supertest": "^3.3.0",
    "updtr": "^2.0.0",
    "watch": "^1.0.2",
    "webpack-cli": "^3.1.0"
  }
}
